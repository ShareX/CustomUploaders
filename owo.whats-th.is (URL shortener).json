{
  "Name": "owo.whats-th.is (URL shortener)",
  "RequestType": "GET",
  "RequestURL": "https://api.whats-th.is/shorten/polr",
  "Arguments": {
    "key": "YOUR_API_KEY",
    "url": "$input$",
    "action": "shorten"
  },
  "ResponseType": "Text",
  "RegexList": [
    "(.+)"
  ],
  "URL": "$regex:1;1$"
}
